{\\r\\n            [name]:value\\r\\n        })\\r\\n    }\\r\\n    render(){\\r\\n        var {tasks}= this.props;\\r\\n        var {filterName,filterStatus}= this.state;\\r\\n        var elmTasks = tasks.map((task,index)=>{\\r\\n            return <TaskItem \\r\\n                        key={index} \\r\\n                        task={task} \\r\\n                        onUpdateStatus={this.props.onUpdateStatus} \\r\\n                        onDelete={this.props.onDelete} \\r\\n                        onEdit={this.props.onEdit}\\r\\n                    />\\r\\n        })\\r\\n        return( \\r\\n            <div>\\r\\n                <table className=\\\"table table-hover\\\">\\r\\n                <thead>\\r\\n                    <tr>\\r\\n                        <th>STT</th>\\r\\n                        <th>Tên</th>\\r\\n                        <th>Trạng Thái</th>\\r\\n                        <th>Hành Động</th>\\r\\n                    </tr>\\r\\n                </thead>\\r\\n                <tbody>\\r\\n                    <tr>\\r\\n                        <td></td>\\r\\n                        <td>\\r\\n                        <input \\r\\n                            type=\\\"text\\\" \\r\\n                            name=\\\"filterName\\\" \\r\\n                            id=\\\"input\\\"\\r\\n                            className=\\\"form-control\\\" \\r\\n                            title=\\\"\\\" \\r\\n                            value={filterName}\\r\\n                            onChange={this.onChange}\\r\\n                        />\\r\\n                        </td>\\r\\n                        <td>\\r\\n                            <select \\r\\n                                name=\\\"filterStatus\\\" \\r\\n                                id=\\\"input\\\" \\r\\n                                className=\\\"form-control\\\" \\r\\n                                required=\\\"required\\\"\\r\\n                                value={filterStatus}\\r\\n